@model IEnumerable<WebsiteQuanAo.Models.DonHang>
<table class="table" style="text-align:center">
    <tr>
        <td>ĐƠN HÀNG</td>
        <td>NGÀY</td>
        <td>TÌNH TRANG</td>
        <td> TỔNG</td>
        <td>THAO TÁC</td>
    </tr>
    <tr>    
    </tr>
    @foreach (var i in Model)
    {
        <tr>
            <td>@i.MaHD</td>
            <td>@i.NgayMua</td>
            <td>@i.TinhTrangDonHang</td>
            <td>@i.TongTien</td>
           
            <td>
                <a href="@Url.Action("chitiet","Account",new { iMaHD = @i.MaHD })" role="button">

                    <button class="btn btn-link" style="color: #807c7c;">
                        Chi tiết
                    </button>

                </a>
            </td>
        </tr>
    }

</table>